  <dd-charts canvasId="ddchart-dom-bar2" onDraw="onDraw"></dd-charts>
</view> -->
<view a:if="{{details.length > 0}}">
    <list>
      <!-- <view slot="header">{{item.areaName}}
      </view> -->
      <view>
        <list-item a:for="{{details}}" key="item-{{index}}" index="{{index}}" onClick="onItemMothClick" last="{{index===(details.length-1)}}" arrow="{{true}}" wrap="{{true}}">                                                      
          <!-- <view style="float:left;width:40%;">{{item.areaName}}
          </view>
          <view style="float:right;width:40%;"></view>
          <view style="float:left;width:20%;">约定:{{item.expected}}亩
          </view>
          <view style="text-alain:left;float:right;width:20%;">核实:{{item.actual}}亩
          </view> -->
          <view>
            {{item.areaName}}（约定：{{item.expected}}亩 核实：{{item.actual}}亩）
          </view>
        </list-item>
      </view>
    </list>
</view>